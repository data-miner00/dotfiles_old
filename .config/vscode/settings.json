{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "diffEditor.ignoreTrimWhitespace": false,
  "d.dubCompiler": "dmd.EXE",
  "d.stdlibPath": "auto",
  "d.dubPath": "C:\\D\\dmd2\\windows\\bin64\\dub.exe",
  "svelte.enable-ts-plugin": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "editor.lineNumbers": "relative",
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.symbolCacheDownload": true,
  "julia.enableTelemetry": true,
  "extensions.ignoreRecommendations": true,
  "vim.vimrc.enable": true,
  "vim.leader": " ",
  "window.commandCenter": false,
  "files.associations": {
    "*.html": "html"
  },
  "discord.enabled": false,
  "editor.fontFamily": "'CaskaydiaMono Nerd Font', Consolas, 'Courier New', monospace",
  "editor.formatOnSave": true,
  "workbench.colorTheme": "Default Light Modern",
  "workbench.colorCustomizations": {
    "button.background": "#eba0ac",
    "button.hoverBackground": "#d38e9a",
    "badge.background": "#eba0ac",
    "badge.foreground": "#fff",
    "textLink.foreground": "#e68b9a",
    "textLink.activeForeground": "#c97180",
    "statusBarItem.remoteBackground": "#eba0ac",
    "statusBarItem.activeBackground": "#d38e9a",
    "activityBarBadge.background": "#eba0ac",
    "activityBar.activeBorder": "#eba0ac",
    "tab.activeBorderTop": "#c97180",
    "terminalCursor.foreground": "#eba0ac",
    "editorGroup.dropBackground": "#eba0ac22",
    "editor.selectionBackground": "#eba0ac55"
  }
}
